{
  "name": "thesalessherpa",
  "version": "1.0.0",
  "description": "Monaco power at HubSpot pricing - Enterprise Sales Intelligence Platform",
  "main": "dist/server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "DEMO_MODE=true ts-node-dev --respawn --transpile-only src/server/app.ts",
    "server:build": "tsc -p tsconfig.server.json",
    "server:start": "node dist/server/app.js",
    "client:dev": "cd src/client && npm start",
    "client:build": "cd src/client && npm run build",
    "build": "npm run client:build",
    "start": "npm run server:start",
    "install:all": "npm install && cd src/client && npm install",
    "db:migrate": "psql $DATABASE_URL -f database/schema.sql",
    "db:seed": "psql $DATABASE_URL -f database/seeds/demo_data.sql",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "typecheck": "tsc --noEmit",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@types/jsforce": "^1.11.6",
    "@vercel/node": "^3.0.21",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsforce": "^2.0.0-beta.28",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "puppeteer": "^21.6.1",
    "redis": "^4.6.11",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "sales",
    "crm",
    "salesforce",
    "ai",
    "relationship-intelligence",
    "sales-enablement",
    "b2b",
    "enterprise"
  ],
  "author": "Ferncliff Partners",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ferncliff/thesalessherpa.git"
  }
}
