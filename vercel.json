{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "src/client/build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/fa/mattedwards/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}